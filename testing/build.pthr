def build = @import("build");
def build_exp = @import("std_exp/build.pthr");


func entry = () #entry -> UI8 {
	@buildSetOutput(build.Output.RUN);
	@buildSetNumThreads(12);
	@buildSetUseStdLib(true);

	const project = new build_exp.Project(new @pthr.ProjectWarningSettings{
		methodCallOnNonMethod        = true,
		deleteMovedFromExpr          = true,
		deleteTriviallyDeletableType = true,
		constexprIfCond              = true,
	});

	project.addSourceFile("test.pthr".* as [Char:*|]);
	// project.addSourceFile("std_exp/std_exp.pthr".* as [Char:*]);
	// @buildAddCPPHeaderFile("test.h".* as [Char:*], true);
	
	return 0;
}