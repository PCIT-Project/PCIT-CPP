def build = @import("build");
def build_exp = @import("std_exp/build.pthr");


func entry = () #entry -> UI8 {
	build_exp.setOutput(build.Output.RUN);
	build_exp.setNumThreads(12);

	// build_exp.addStdLib("../extern/Panther-std/std/");
	// build_exp.addStdLib("../../Panther-std/std");


	const main_project = new build_exp.Project(
		"./".* as [Char:*],
		new @pthr.ProjectWarningSettings{
			methodCallOnNonMethod        = true,
			deleteMovedFromExpr          = true,
			deleteTriviallyDeletableType = true,
			constexprIfCond              = true,
		}
	);
	main_project.addSourceFile("test.pthr");
	// main_project.addSourceFile("std_exp/std_exp.pthr");

	// @buildAddCHeaderFile("test.h", true);
	
	return 0;
}