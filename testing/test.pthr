

interface Inter = {
	func err_test = () -> Int <UI8>;
}



type Thing = struct {
	func err_test_impl = () -> Int <UI8> {
		error 12;
	}


	impl Inter {
		err_test = err_test_impl,
	}
}





func entry = () #entry -> UI8 {
	var err_code: UI8 = 0;

	var thing = new Thing();

	var inter: Inter*| = &thing as Inter*|;

	_ = try inter.err_test() else<code> {->handle:Int
		err_code = move code;
		return->handle 0;
	};
	
	return move err_code;
}