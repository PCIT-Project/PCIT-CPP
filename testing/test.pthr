interface IGetNum = #polymorphic {
	func get_num = () -> Int;
}

type Thing = struct {
	impl IGetNum{
		get_num = () -> Int { return 12; }
	}
}



func get_num = (foo: impl($$:IGetNum)) -> Int {
	return foo.get_num();
}


func create_thing_get_num = () -> impl(Thing:IGetNum) {
	return new Thing() as impl(Thing:IGetNum);
}


