
def NEW_FEATURE: Bool = true;


when(NEW_FEATURE){
	type Struct = struct <{TYPE: Type, VAL: TYPE}> {
		def VALUE: TYPE = VAL;
	}

	func entry = () #entry -> UI8 {
		return Struct<{UI8, 12}>.VALUE;
	}

}else{
	type Struct = struct <{T: Type}> {
		def VALUE: T = 12;
	}

	func entry = () #entry -> UI8 {		
		return Struct<{UI8}>.VALUE;
	}
}
