

type Vec2 = struct <{T: Type, VALUE: Char = 'd'}> {
	var x: T;
	var y: T;

	func get_sum = (this) #rt -> T {
		return @fadd<{T,}>(this.x, this.y);
	}


	func as = (this) #rt -> Vec2<{F64, VALUE}> {
		return new Vec2<{F64, VALUE}>{
			x = this.x as F64,
			y = this.y as F64,
		};
	}

}






func entry = () #entry -> UI8 {
	const vec = new Vec2<{F32, 'a'}>{
		x = 12.4,
		y = 19.6,
	};


	const vec_f64: Vec2<{F64}> = copy vec;
	
	return vec_f64.get_sum() as UI8;
}

