interface Shape = {
	func area = (this) -> USize;

	impl Rectangle {
		area = (this) -> USize { return this.width * this.height; }
	}
}



type Rectangle = struct {
	var width: USize;
	var height: USize;
}



func get_area = (shape: Shape) -> USize {
	return shape.area();
}


func entry = () #entry -> UI8 {
	var rect = new Rectangle{width = 2, height = 4};
	
	return get_area(rect) as UI8;
}







