def atomic = @import("atomic.pthr");


func test = () -> UI32 {
	var num: UI32 = 9;

	var num_ref = new atomic.AtomicRef<{UI32}>(&num);

	num_ref.store<{atomic.AtomicOrdering.MONOTONIC}>(14);

	[_, _] = num_ref.compareExchange(14, 12);

	return num_ref.load();
}



func entry = () #entry -> UI8 {
	def NUM: UI32 = test();
	return NUM as UI8;
}