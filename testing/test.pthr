

type Thing = struct {
	// func new = (num: Int) -> (output: Thing) {
	// 	output.num = copy num;
	// 	return...;
	// }

	func copy = delete;

	func copy = (this) -> (output: Thing) {
		output.num = this.num;
		return...;
	}


	func == = (this, rhs: Thing) -> Bool {
		return this.num == rhs.num;
	}


	var num: Int;
}


func entry = () #entry -> UI8 {
	var foo = new Thing{num = 12};
	var bar = new Thing{num = 14};
	
	return (foo == bar) as UI8;
}
