

type Vec2 = struct {
	var x: F32;
	var y: F32;
}


def GLOBAL: Vec2 = get_vec2();
def GLOBAL_Y: F32 = GLOBAL.y;

func get_vec2 = () -> (output: Vec2) {
	output = new Vec2{
		x = 12.0,
		y = 3.5,
	};
	return...;
}



func make_ones = (vec2: Vec2 mut) -> Void {
	vec2.x = 1.0;
	vec2.y = 1.0;
}




func entry = () #entry -> UI8 {
	var vec2: Vec2 = get_vec2();

	// make_ones(vec2);

	return @fToUI<{F32, UI8}>(@fdiv<{F32, false}>(vec2.x, GLOBAL_Y));
}
